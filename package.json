{
  "name": "@xquare/utils",
  "version": "1.1.0",
  "description": "XQUARE typescript 통합 유틸",
  "main": "dist/index.js",
  "publishConfig": {
    "main": "dist/index.js",
    "types": "dist/index.d.ts"
  },
  "scripts": {
    "test": "jest",
    "test:cov": "jest --coverage",
    "format": "prettier --write \"src/**/*.ts\" \"dist/**/*.d.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TEAM-XQUARE/ts-utils.git"
  },
  "keywords": [
    "util",
    "xquare",
    "dsm"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TEAM-XQUARE/ts-utils/issues"
  },
  "homepage": "https://github.com/TEAM-XQUARE/ts-utils#readme",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.0",
    "jest": "^26.6.3",
    "prettier": "^2.3.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "dependencies": {
    "immer": "^9.0.2"
  }
}
